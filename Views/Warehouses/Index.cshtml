@model IEnumerable<ExsitecParonAB.Models.Warehouse>

<h2>Färdigvarulager</h2>
<hr />


<table class="table">
    <tr>
        <th>
            Lagernr.
        </th>
        <th>
            Stad
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.warehouseNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.city)
            </td>
            <td>
                @Html.ActionLink("Ändra", "Edit", new { id = item.warehouseNo }) |
                @Html.ActionLink("Radera", "Delete", new { id = item.warehouseNo })
            </td>
        </tr>
    }

</table>
<p>
    @Html.ActionLink("Lägg till", "Create")
</p>
